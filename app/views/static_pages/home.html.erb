<%= form_tag("/search", method: "post") do %>
  <%= label_tag(:chicken, "Search for:") %>
  <%= text_field_tag(:chicken) %>
  <%= submit_tag "Search", id: "search_button" %>
<% end %>



<% if @bars %>
	<% @bars.each do |bar| %>
		<%= link_to venue_path(bar) do %>
		 	<div class="venue">
		 	<ul>
 		 		<li><%= bar.name %></li>
 		 		<li><%= bar.address %></li>
 			</ul>
 			</div>
		<%end%>
	<% end %>
<%end%>

