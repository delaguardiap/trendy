<%= form_tag("/search", method: "post") do %>
  <%= label_tag(:chicken, "Search for:") %>
  <%= text_field_tag(:chicken) %>
  <%= submit_tag("Search") %>

<% end %>

<%b = 0%>

<% if @bars %>
	<% @bars.each do |bar| %>
		<%b = bar%>
		<%= link_to "#{bar.name}, #{bar.address} #{bar.rating}", venue_path(bar) %><br>
	<% end %>
	<%= getTweets b %>
<%end%>
